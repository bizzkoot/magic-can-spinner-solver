{
  "extends": "@parcel/config-default",
  "entries": ["index.html"],
  "distDir": "docs",
  "transformers": {
    "*.{js,mjs,jsx,cjs,ts,tsx}": ["@parcel/transformer-js"],
    "*.vue": ["@parcel/transformer-vue"],
    "*.html": ["@parcel/transformer-html"]
  },
  "reporters": ["@parcel/reporter-cli"],
  "optimizers": {
    "*.js": ["@parcel/optimizer-terser"]
  }
}
