{
  "name": "bead-puzzle-solver",
  "version": "1.0.0",
  "description": "Bead Puzzle Solver",
  "private": true,
  "source": "index.html",
  "scripts": {
    "clean": "rm -rf .parcel-cache dist docs node_modules/.cache",
    "build": "parcel build --target default",
    "start": "parcel index.html --port 3000"
  },
  "dependencies": {
    "@vue/runtime-dom": "^3.3.4",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "3.5.13",
    "@parcel/transformer-vue": "^2.9.3",
    "parcel": "^2.9.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "targets": {
    "default": {
      "source": ["index.html"],
      "distDir": "docs",
      "publicUrl": "/bead-puzzle-solver/",
      "isLibrary": false
    }
  }
}
