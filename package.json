{
  "name": "bead-puzzle-solver",
  "version": "1.0.0",
  "description": "Bead Puzzle Solver",
  "private": true,
  "source": "index.html",
  "scripts": {
    "clean": "rm -rf .parcel-cache dist docs",
    "build": "parcel build",
    "build:prod": "parcel build --no-cache",
    "start": "parcel index.html --port 3000"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "@vue/runtime-dom": "^3.3.4"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.3.4",
    "@parcel/transformer-vue": "^2.9.3",
    "parcel": "^2.9.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "targets": {
    "default": {
      "source": ["index.html"],
      "distDir": "docs",
      "publicUrl": "./bead-puzzle-solver/",
      "isLibrary": false,
      "context": "browser",
      "engines": {
        "browsers": [
          "last 2 Chrome versions"
        ]
      }
    }
  }
}
